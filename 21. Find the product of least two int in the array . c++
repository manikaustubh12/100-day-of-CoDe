// find the product of pair of two least element
#include <iostream>
#include<bits/stdc++.h>
typedef long long km;
using namespace std;

int main() {
    km n; cin>>n;
    // km z; cin>>z;
    // km mini=INT_MAX;
    vector<km> v(n);
    for(km i=0;i<n;i++){
        cin>>v[i];
        // if(v[i]<mini)
        //     mini=v[i];
    }
    sort(v.begin(),v.end());
    km mul=v[0]*v[1];
    // for(km i=0;i<n;i++){
        
    // }
    cout<<mul;
    
    return 0;
}
